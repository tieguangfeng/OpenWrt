config config_subscribe
	option enabled '1'
	option name 'AirSetting'
	option address 'https://AirSetting'
	option sub_ua 'clash.meta'
	option sub_convert '1'
	list ex_keyword '10倍'
	list ex_keyword '剩余'
	list ex_keyword '到期'
	list ex_keyword '紧急'
	list ex_keyword '官网'
	list ex_keyword '更新'
	list ex_keyword '客服'
	list ex_keyword '电报'
	list ex_keyword '提现'
	list ex_keyword '显示'
	list ex_keyword '一元'
	list ex_keyword '机场'
	option convert_address 'https://pbus.tieguangfeng.workers.dev/sub'
	option template '0'
	option custom_template_url 'https://raw.githubusercontent.com/tieguangfeng/OpenWrt/main/ClashCustomize.ini'
	option emoji 'true'
	option udp 'true'
	option skip_cert_verify 'false'
	option sort 'false'
	option node_type 'false'
	option rule_provider 'false'

